        <Rule>
            <PolygonSymbolizer fill="#d9d0c9" />
            <LineSymbolizer stroke="#c1b3a4" />
        </Rule>
    </Style>
    <Style name="roads">
        <Rule>
            <LineSymbolizer stroke="#808080" />
        </Rule>
    </Style>
</Map>
